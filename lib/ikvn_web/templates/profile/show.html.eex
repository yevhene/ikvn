<h2><%= gettext "Profile" %></h2>

<ul>
  <li>
    <strong><%= gettext "Nickname" %>:</strong>
    <%= @user.nickname %>
  </li>
  <li>
    <strong><%= gettext "Email" %>:</strong>
    <%= @user.email %>
  </li>
</ul>

<span><%= link gettext("Edit"), to: Routes.profile_path(@conn, :edit) %></span>

<hr />
<span><%= link gettext("Back"), to: "/" %></span>
